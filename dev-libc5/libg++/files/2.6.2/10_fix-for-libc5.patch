+#include "libioP.h"
+
+#ifdef __STDC__
+#include <stdarg.h>
+#else
+#include <varargs.h>
+#endif
+
+int
+fprintf
+#ifdef __STDC__
+  (_IO_FILE *fp, const char* format, ...)
+#else
+(fp, format, va_alist) _IO_FILE *fp; char *format; va_dcl
+#endif
+{
+  int ret;
+  va_list args;
+  CHECK_FILE(fp, -1);
+  _IO_va_start(args, format);
+  ret = _IO_vfprintf(fp, format, args);
+  va_end(args);
+  return ret;
+}
